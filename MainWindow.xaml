<Window x:Class="Cursovoy_project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cursovoy_project"
        mc:Ignorable="d"
        Title="MainWindow" Height="520" Width="600">
    <Window.Background>
        <LinearGradientBrush>
            <GradientStop Color="#0AC672" Offset="1" />
            <GradientStop Color="#064DBA" Offset="0.5" />
            <GradientStop Color="#E2DB05" Offset="0" />
        </LinearGradientBrush>
    </Window.Background>

    <ContentPresenter x:Name="OutputView"></ContentPresenter>
</Window>
